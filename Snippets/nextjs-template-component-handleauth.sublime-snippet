import React, {Component} from 'react'
import {auth} from './$2'



class allroles extends Component{
  render(){
    return (
      <div className="allroles-wrapper">
        allroles
      </div>
    )
  }
}


allroles.getInitialProps = async (ctx) => {
	let token = (await auth(ctx)) || null
  return {token}
};


export default allroles
